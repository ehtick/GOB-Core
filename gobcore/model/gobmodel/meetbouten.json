{
    "meetbouten": {
        "version": "0.1",
        "description": "De meetgegevens van bouten in de Amsterdamse panden.",
        "abbreviation": "MBN",
        "collections": {
            "meetbouten": {
                "version": "0.1",
                "abbreviation": "MBT",
                "entity_id": "identificatie",
                "attributes": {
                    "identificatie": {
                        "type": "GOB.String",
                        "description": "Unieke identificatie van de meetbout"
                    },
                    "nabij_nummeraanduiding": {
                        "type": "GOB.Reference",
                        "description": "Een adres in de nabijheid van de meetbout",
                        "ref": "bag:nummeraanduidingen"
                    },
                    "locatie": {
                        "type": "GOB.String",
                        "description": "Beschrijving van de locatie van de meetbout, bijvoorbeeld 'in gemeenschappelijke muur'"
                    },
                    "status": {
                        "type": "GOB.JSON",
                        "description": "Status van de meetbout (1=actueel, 2=niet te meten, 3=vervallen)",
                        "attributes": {
                            "code": {
                                "type": "GOB.Integer"
                            },
                            "omschrijving": {
                                "type": "GOB.String"
                            }
                        }
                    },
                    "vervaldatum": {
                        "type": "GOB.Date",
                        "description": "Datum waarop de meting heeft plaatsgevonden"
                    },
                    "merk": {
                        "type": "GOB.JSON",
                        "description": "Merk",
                        "attributes": {
                            "code": {
                                "type": "GOB.Integer"
                            },
                            "omschrijving": {
                                "type": "GOB.String"
                            }
                        }
                    },
                    "x_coordinaat_muurvlak": {
                        "type": "GOB.Decimal",
                        "description": "X-coördinaat muurvlak"
                    },
                    "y_coordinaat_muurvlak": {
                        "type": "GOB.Decimal",
                        "description": "Y-coördinaat muurvlak"
                    },
                    "windrichting": {
                        "type": "GOB.String",
                        "description": "Windrichting"
                    },
                    "ligt_in_bouwblok": {
                        "type": "GOB.Reference",
                        "description": "Het bouwblok waarbinnen de meetbout ligt",
                        "ref": "gebieden:bouwblokken"
                    },
                    "ligt_in_buurt": {
                        "type": "GOB.Reference",
                        "description": "De buurt waarbinnen de meetbout ligt",
                        "ref": "gebieden:buurten"
                    },
                    "ligt_in_stadsdeel": {
                        "type": "GOB.Reference",
                        "description": "Het stadsdeel waarbinnen de meetbout ligt",
                        "ref": "gebieden:stadsdelen"
                    },
                    "geometrie": {
                        "type": "GOB.Geo.Point",
                        "srid": "RD",
                        "description": "Geometrische ligging van de meetbout"
                    },
                    "publiceerbaar": {
                        "type": "GOB.Boolean",
                        "description": "Publiceerbaar ja of nee"
                    }
                },
                "api": {
                    "filters": [
                        {
                            "field": "publiceerbaar",
                            "op": "==",
                            "value": true
                        }
                    ]
                }
            },
            "metingen": {
                "version": "0.1",
                "abbreviation": "MTG",
                "entity_id": "identificatie",
                "attributes": {
                    "identificatie": {
                        "type": "GOB.String",
                        "description": "Unieke identificatie van de meting"
                    },
                    "hoort_bij_meetbout": {
                        "type": "GOB.Reference",
                        "description": "De meetbout waarop de meting is gedaan",
                        "ref": "meetbouten:meetbouten"
                    },
                    "datum": {
                        "type": "GOB.Date",
                        "description": "Datum waarop de meting heeft plaatsgevonden"
                    },
                    "type_meting": {
                        "type": "GOB.Character",
                        "description": "Type meting (N=nulmeting, H=herhalingsmeting, T=tussentijdse meting, S=schatting)"
                    },
                    "wijze_van_inwinnen": {
                        "type": "GOB.JSON",
                        "description": "Wijze waarop de meting is ingewonnen",
                        "attributes": {
                            "code": {
                                "type": "GOB.Integer"
                            },
                            "omschrijving": {
                                "type": "GOB.String"
                            }
                        }
                    },
                    "hoogte_tov_nap": {
                        "type": "GOB.Decimal",
                        "description": "Gemeten hoogte van de meetbout tov NAP"
                    },
                    "zakking": {
                        "type": "GOB.Decimal",
                        "description": "Zakking van de meting t.o.v. vorige meting (mm)"
                    },
                    "refereert_aan_referentiepunten": {
                        "type": "GOB.ManyReference",
                        "description": "De referentiepunten waar de metingen aan worden opgehangen",
                        "ref": "meetbouten:referentiepunten"
                    },
                    "zakkingssnelheid": {
                        "type": "GOB.Decimal",
                        "description": "Zakkingssnelheid van de meetbout (mm/​jaar) sinds de meting"
                    },
                    "zakking_cumulatief": {
                        "type": "GOB.Decimal",
                        "description": "Cumulatieve zakking sinds de plaatsing van de meetbout"
                    },
                    "is_gemeten_door": {
                        "type": "GOB.String",
                        "description": "ID Van de onderneming die de meting heeft uitgevoerd"
                    },
                    "hoeveelste_meting": {
                        "type": "GOB.Integer",
                        "description": "Hoeveelste meting van de meetbout"
                    },
                    "aantal_dagen": {
                        "type": "GOB.Integer",
                        "description": "Het aantal dagen sinds de vorige meting"
                    },
                    "publiceerbaar": {
                        "type": "GOB.Boolean",
                        "description": "Publiceerbaar ja of nee"
                    }
                },
                "api": {
                    "filters": [
                        {
                            "field": "publiceerbaar",
                            "op": "==",
                            "value": true
                        }
                    ]
                }
            },
            "referentiepunten": {
                "version": "0.1",
                "abbreviation": "RPT",
                "entity_id": "identificatie",
                "attributes": {
                    "identificatie": {
                        "type": "GOB.String",
                        "description": "Unieke identificatie van de meting"
                    },
                    "nabij_nummeraanduiding": {
                        "type": "GOB.Reference",
                        "description": "Een adres in de nabijheid van het referentiepunt",
                        "ref": "bag:nummeraanduidingen"
                    },
                    "locatie": {
                        "type": "GOB.String",
                        "description": "Beschrijving van de locatie van het referentiepunt bijv 'nabij noordoostelijke gevelhoek'."
                    },
                    "hoogte_tov_nap": {
                        "type": "GOB.Decimal",
                        "description": "Hoogte van het referentiepunt t.o.v. NAP"
                    },
                    "datum": {
                        "type": "GOB.Date",
                        "description": "Datum van plaatsing van het referentiepunt."
                    },
                    "status": {
                        "type": "GOB.JSON",
                        "description": "Status van het referentiepunt (1=actueel, 2=niet te meten, 3=vervallen)",
                        "attributes": {
                            "code": {
                                "type": "GOB.Integer"
                            },
                            "omschrijving": {
                                "type": "GOB.String"
                            }
                        }
                    },
                    "vervaldatum": {
                        "type": "GOB.Date",
                        "description": "Vervaldatum van het referentiepunt."
                    },
                    "merk": {
                        "type": "GOB.JSON",
                        "description": "Merk van het referentiepunt",
                        "attributes": {
                            "code": {
                                "type": "GOB.Integer"
                            },
                            "omschrijving": {
                                "type": "GOB.String"
                            }
                        }
                    },
                    "x_coordinaat_muurvlak": {
                        "type": "GOB.Decimal",
                        "description": "X-coördinaat muurvlak"
                    },
                    "y_coordinaat_muurvlak": {
                        "type": "GOB.Decimal",
                        "description": "Y-coördinaat muurvlak"
                    },
                    "windrichting": {
                        "type": "GOB.String",
                        "description": "Windrichting"
                    },
                    "ligt_in_bouwblok": {
                        "type": "GOB.Reference",
                        "description": "Het bouwblok waarbinnen het referentiepunt ligt",
                        "ref": "gebieden:bouwblokken"
                    },
                    "ligt_in_buurt": {
                        "type": "GOB.Reference",
                        "description": "De buurt waarbinnen het referentiepunt ligt",
                        "ref": "gebieden:buurten"
                    },
                    "ligt_in_stadsdeel": {
                        "type": "GOB.Reference",
                        "description": "Het stadsdeel waarbinnen het referentiepunt ligt",
                        "ref": "gebieden:stadsdelen"
                    },
                    "geometrie": {
                        "type": "GOB.Geo.Point",
                        "srid": "RD",
                        "description": "Geometrische ligging van de meetbout"
                    },
                    "is_nap_peilmerk": {
                        "type": "GOB.Reference",
                        "description": "Het NAP-peilmerk dat het referentiepunt kan zijn.",
                        "ref": "nap:peilmerken"
                    },
                    "publiceerbaar": {
                        "type": "GOB.Boolean",
                        "description": "Publiceerbaar ja of nee"
                    }
                },
                "api": {
                    "filters": [
                        {
                            "field": "publiceerbaar",
                            "op": "==",
                            "value": true
                        }
                    ]
                }
            },
            "rollagen": {
                "version": "0.1",
                "abbreviation": "RLG",
                "entity_id": "identificatie",
                "attributes": {
                    "identificatie": {
                        "type": "GOB.String",
                        "description": "Unieke identificatie van de rollaag"
                    },
                    "is_gemeten_van_bouwblok": {
                        "type": "GOB.Reference",
                        "description": "Het bouwblok waarvan de rollaag is gemeten",
                        "ref": "gebieden:bouwblokken"
                    }
                }
            }
        }
    }
}